{
  "buildCommand": "npm run build",
  "functions": {
    "src/app/api/**/*.ts": {
      "maxDuration": 30
    },
    "src/app/api/files/[filename]/route.ts": {
      "maxDuration": 15,
      "memory": 512
    }
  },
  "env": {
    "NODE_ENV": "production",
    "VERCEL": "1",
    "VERCEL_SKIP_MIGRATIONS": "1"
  }
}